function solve(arr) {

let result = arr[0][0];
for(let  i = 0;i<  arr.length; i++ ) {
    for(let j = 0; j< arr.length; j++) {
        if(arr[i][j] > result) {
            result = arr[i][j]
        }
    }
}
return result
}


console.log(
    solve([[3, 5, 7, 12],
        [-1, 4, 33,
            2],
        [8, 3, 0, 4]])
)

->33
