function solve(num) {
    for(let i = 0; i< num.length; i++) {
        let currentNumber = num[i]
        let result  = num[i].toString().split('').reverse().join('')

        if(Number(result) === currentNumber) {
            console.log('true')
        } else {
            console.log('false')
        }

    }
}

console.log(
    solve([123,323,421,121])
)


->false
true
false
true
                 
