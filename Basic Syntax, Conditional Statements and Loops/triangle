function triangle(n) {
    let output = "";
    for(let row = 1; row <= n; row++) {
        for(let col = 1; col<= row; col++) {
            output += row  + " ";
        }
      output += '\n';
    }
    console.log(output)
}

//Another way:
function trianle(n) {
     for(let row = 1; row <= n; row++) {
         console.log(`${row} `.repeat(row))
     }

}

triangle(3)
