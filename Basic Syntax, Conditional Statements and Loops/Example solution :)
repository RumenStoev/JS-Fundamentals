//Sort numbers !!!
function solve(...input) {
  let totalResult=0;
   let result =  input.sort((a,b) => {
    return b - a
   })
 for (i=0;i<result.length;i++)
{
  document.write(result[i] + "<br >");
}

} 

console.log(
  solve(3,1,2)
)

console.log(
  solve(-2,1,3)
)
console.log(
  solve(0,0,2)
)
