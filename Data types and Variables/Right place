//In this function use method string "replace" and operator "?"

function solve(str,char,result) {
  let res = str.replace("_",char)
  let output = res === result ? "Mached" : "Not Mached"

 return(output)
}

onsole.log(
  solve("Str_ng","l","Strong")
)
->No Matched

console.log(
  solve("Str_ng","i","String")
)

->Mached

